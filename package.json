{
  "name": "printani",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "clean": "rimraf lib",
    "build": "npm run clean && npm run tsc --",
    "watch": "npm run build -- -w",
    "dev:ios": "npm run build && concurrently -r 'npm run watch' 'react-native run-ios'",
    "dev:android": "npm run build && concurrently -r 'npm run watch' 'react-native run-android'",
    "lint": "tslint app/**/*.tsx",
    "lint:fix": "tslint app/**/*.tsx --fix"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "react": "^16.3.0-alpha.1",
    "react-native": "0.54.2",
    "recompact": "^3.3.0",
    "styled-components": "^3.2.3",
    "tslint-config-airbnb": "^5.7.0"
  },
  "devDependencies": {
    "@types/jest": "^22.2.0",
    "@types/react": "^16.0.40",
    "@types/react-native": "^0.52.18",
    "@types/react-test-renderer": "^16.0.1",
    "babel-jest": "23.0.0-alpha.0",
    "babel-preset-react-native": "4.0.0",
    "jest": "22.4.2",
    "react-test-renderer": "^16.3.0-alpha.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
